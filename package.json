{
  "name": "drinkinggame",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"server,client\" -c \"blue,green\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm --prefix server start",
    "dev:client": "npm --prefix client run dev",
    "install:all": "npm install && npm --prefix server install && npm --prefix client install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
